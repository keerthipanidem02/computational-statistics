import pandas as pd
def rankco(df):
    x=df.x
    y=df.y
    r1={}
    x1=sorted(x,reverse=True)
    y1=sorted(y,reverse=True)
    r=1
    n=len(x)
#ranks for x
    for i in range(len(x1)):
        if x1[i] in r1.keys():
            r1[x1[i]]=(r1[x1[i]]+r)
        else:
            r1[x1[i]]=r
        r+=1
    for i in r1.keys():
            r1[i]=(r1[i])/(x1.count(i))

#ranks for y         
    r=1
    r2={}
    for i in range(len(y1)):
        if y1[i] in r2.keys():
            r2[y1[i]]=(r2[y1[i]]+r)
           
           
        else:
            r2[y1[i]]=r
        r+=1
    for i in r2.keys():
            r2[i]=(r2[i])/(y1.count(i))
    df['ranks_of_x']=[r1[i] for i in x]
    df['ranks_of_y']=[r2[i] for i in y]
    df['diff']=df['ranks_of_x']-df['ranks_of_y']
    df['diffsq']=df['diff']**2
    sigma_d=sum(df['diffsq'])
#printing dataframe
    print(df)
    l=[]
    cf=0
    x=list(x)
    y=list(y)
    for i in x:
        if x.count(i)>0:
            l.append(x.count(i))
    k=[]
    for j in y:
        if y.count(j)>0:
            k.append(y.count(j))
    for i in l:
        cf=cf+i*(i**2-1)/12
    for j in k:
        cf=cf+j*(j**2-1)/12
    d=6*(sigma_d+cf)/(n*(n**2-1))
    r1=1-d
    print(r1)
           
           
   
df=pd.DataFrame({'x':[110,100,140,120,80,90,90],"y":[70,60,80,90,10,20,10]})
rankco(df)
